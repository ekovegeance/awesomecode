{
  "name": "vngne-theme",
  "displayName": "vngne Theme",
  "description": "vngne theme for VS Code",
  "repository": {
    "type": "git",
    "url": "https://github.com/ekovegeance/vngne-theme"
  },
  "version": "0.0.5",
  "publisher": "ekovegeance",
  "sponsor": {
    "url": "https://github.com/sponsors/ekovegeance"
  },
  "license": "MIT",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#18181b",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "vngne",
    "dark"
  ],
  "contributes": {
    "themes": [
      {
        "label": "vngne Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/vngne-color-theme.json"
      }
    ],
    "iconThemes": [
      {
        "id": "vngne-icon-theme",
        "label": "vngne Icon Theme",
        "path": "./icons/vngne-icon-theme.json"
      }
    ],
    "configurationDefaults": {
      "[vngne]": {
        "workbench.colorTheme": "vngne Theme",
        "workbench.iconTheme": "vngne Icon Theme"
      }
    }
  }
}
